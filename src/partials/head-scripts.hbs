{{#with site.keys.googleAnalytics}}
<!-- Google Analytics -->
<script id="google-script">
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

ga('create', '{{this}}', 'auto')
ga('send', 'pageview')
</script>
{{#contains 'Page Not Found' @root.page.title}}
<script>
  var url = document.referrer
  if (url == '') { url = 'No referrer' }
  ga('send', 'event', '404', url /* page as action */)
</script>
{{/contains}}
<!-- End Google Analytics -->
{{/with}}
<script src="https://kit.fontawesome.com/097f7829d8.js" crossorigin="anonymous"></script>
{{#if (eq page.attributes.type 'howto')}}
<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "HowTo",
    "name": "{{{page.title}}}",
    "description": "{{{page.description}}}",
    "totalTime": "{{page.attributes.time}}"
  }
</script>
{{/if}}
{{#if (eq page.attributes.type 'faq')}}
<script src="{{{uiRootPath}}}/js/vendor/faq.js"></script>
{{/if}}
